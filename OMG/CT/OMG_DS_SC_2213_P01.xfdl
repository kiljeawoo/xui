		this.ds_svcAuth.addRow();
		this.ds_svcAuth.setColumn(this.ds_svcAuth.rowposition, "ELT_AUTH_DSC", "11");
		chk_CONTRACT_AUTH = "1";
	}
	// 12: 공통
