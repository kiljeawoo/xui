		this.ds_svcAuth.addRow();
		this.ds_svcAuth.setColumn(this.ds_svcAuth.rowposition, "ELT_AUTH_DSC", "99");
		chk_CONTRACT_AUTH = "1";
	}
	// A1: 계약체결 최종서명 권한
	if (this.chk_CONTRACT_AUTH_A1.value == "1" && this.chk_CONTRACT_AUTH_A1.enable) {
