//메인 계약서 위치 체크
this.position_chk = function()
{
	//sts_position (계약서 기준 위치)
	mnCtrwKd = nexacro.toNumber(this.sts_position.top)+nexacro.toNumber(this.sts_position.height) + 20 ;

	if (this.gfn_isNull( this.div_mnCtrwKd.rdo_mnCtrwKdC.value)) {
		this.div_mnCtrwKd41.set_top(mnCtrwKd);
		mnCtrwKd = nexacro.toNumber(this.div_mnCtrwKd41.top)+nexacro.toNumber(this.div_mnCtrwKd41.height) + 20 ;
	}

	if (this.div_mnCtrwKd.rdo_mnCtrwKdC.value == 141) {
		this.div_mnCtrwKd41.set_top(mnCtrwKd);
		this.div_mnCtrwKd41.set_height(1100); // 높이설정
		mnCtrwKd = nexacro.toNumber(this.div_mnCtrwKd41.top) + nexacro.toNumber(this.div_mnCtrwKd41.height) +20 ;
	} else if (this.div_mnCtrwKd.rdo_mnCtrwKdC.value == 143) {
		this.div_mnCtrwKd43.set_top(mnCtrwKd);
		this.div_mnCtrwKd43.set_height(890); // 높이설정
		mnCtrwKd = nexacro.toNumber(this.div_mnCtrwKd43.top) + nexacro.toNumber(this.div_mnCtrwKd43.height) +20 ;
	} else if (this.div_mnCtrwKd.rdo_mnCtrwKdC.value == 144) {
		this.div_mnCtrwKd44.set_top(mnCtrwKd);
		this.div_mnCtrwKd44.set_height(748); // 높이설정
		mnCtrwKd = nexacro.toNumber(this.div_mnCtrwKd44.top) + nexacro.toNumber(this.div_mnCtrwKd44.height) +20 ;
	} else if (this.div_mnCtrwKd.rdo_mnCtrwKdC.value == 145) {
		this.div_mnCtrwKd45.set_top(mnCtrwKd);
		this.div_mnCtrwKd45.set_height(890); // 높이설정
		mnCtrwKd = nexacro.toNumber(this.div_mnCtrwKd45.top) + nexacro.toNumber(this.div_mnCtrwKd45.height) +20 ;
	} else if (this.div_mnCtrwKd.rdo_mnCtrwKdC.value == 195 || this.div_mnCtrwKd.rdo_mnCtrwKdC.value == 196  
		|| this.div_mnCtrwKd.rdo_mnCtrwKdC.value == 197 || this.div_mnCtrwKd.rdo_mnCtrwKdC.value == 198) {
		this.div_mnCtrwKd90.set_top(mnCtrwKd);
		this.div_mnCtrwKd90.set_height(890); // 높이설정
		mnCtrwKd = nexacro.toNumber(this.div_mnCtrwKd90.top) + nexacro.toNumber(this.div_mnCtrwKd90.height) +20 ;
	}
	this.position_add_chk();
}

