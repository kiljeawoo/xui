	//new_elt  ==  true   신규 계약서 작성시
	//new_elt  ==  false  계약서 수정시
	this.new_elt = this.gfn_isNull(this.parent.edt_chk);  //

	if (!this.new_elt) {
		fv_ctrwChgObjChk = "1";
		fv_CtrwChg = this.parent.edt_CtrwChg.value;
