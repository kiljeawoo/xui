 this.btn_save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{

    // alert('btn_save_onclick');
    
	var dsParams = "";
	var params = "";

	//trace("[ Params ] >>>>>> " + params);	
	dsParams = "ds_m=ds_m ";

	var sSvcID        = "saveCertInfoList";//통신아이디
