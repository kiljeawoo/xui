	var sCallbackFunc = "fn_callback";//콜백
	var tranType      = "S"; // U:저장/삭제, S:조회
	
	//API 호출 
	this.gfn_callService(sSvcID, sURL, sInDatasets, sOutDatasets, sArgument, sCallbackFunc, tranType, true);
}

