	if (application.confirm(msg)) {
		var params = "";

		//상품 기본
		this.addRowFile();	//이미지 파일 데이터셋에 저장
		this.ds_wrs_rg_req_filter.setColumn(0,"APV_RQ_NA_BZPLC",TRPL_C);
