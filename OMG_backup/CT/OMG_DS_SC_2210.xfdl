this.search = function(){


	if(this.fn_validationCheck()){

		var cboContDay = this.gfn_nullToEmpty(this.div_search.cbo_contDay.value);  //계약시작0, 만료일1
		var calFrom    = this.gfn_nullToEmpty(this.div_search.cal_from.value);  //조회시작일
