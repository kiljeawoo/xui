				var edt_psgDd = edt_psgDd.split("|");
				if(edt_psgDd.length == 2) {
					this.edt_psgDd.set_value(edt_psgDd[0]);
					this.edt_psgDd00.set_value(edt_psgDd[1]);
				}else if(edt_psgDd.length == 1) {
