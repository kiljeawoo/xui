		trace(this.ds_addEltCtrwDtl02.saveXML())	;
			
		var etc1 = this.ds_addEltCtrwDtl02.getColumn(0,"ETC1") ;	
		
		trace("edt_psgDd 1=>>  " +edt_psgDd)	;
		
			/**
			if(edt_psgDd == "초"   || edt_psgDd == "말"  ){
					this.edt_psgDd.set_value(edt_psgDd);
