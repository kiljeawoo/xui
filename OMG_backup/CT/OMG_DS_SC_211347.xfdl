	{
		var nCheck = this.ds_sctrwAllXpcPmtnCst.getColumn(i, "ISCHECKED");
		if (nCheck == 1) 
		{
			this.parent.ds_sctrwAllXpcPmtnCst.deleteRow(i);
			this.ds_sctrwAllXpcPmtnCst.deleteRow(i);
		}
	}

