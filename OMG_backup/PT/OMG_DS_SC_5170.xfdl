		this.Div01.txa_arr02.set_value(this.policyPrivacy);
	}

	this.ds_userInfo.setColumn(0,"SMS_RMS_YN","N");
	this.ds_userInfo.setColumn(0,"EMAIL_RMS_YN","N");
}

this.fn_afterFormOnload = function()
{
