this.checkParent = function(init)
{
	if (this.parent.name == "OMG_DS_SC_5100") {
		if (init) {
			// 초기 실행시에 데이터 가져옴
			this.parent._getParentData();
			this.ds_bzplc.copyData(this.parent.ds_bzplc);
		}
		return true;
	}
	return false;
}

this.fn_close = function()
{
	if (this.checkParent()) {
		this.parent._closeParent();
	} else {
		this.close();
