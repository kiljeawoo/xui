	// P: PDA 검수
	if (this.Div05.chk_PDA_AUTH.value == "Y") {
		this.ds_svcAuth.addRow();
		this.ds_svcAuth.setColumn(this.ds_svcAuth.rowposition, "SVC_ID", "P");
