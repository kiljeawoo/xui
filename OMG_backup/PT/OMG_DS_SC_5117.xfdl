this.CheckBox04_onchanged = function(obj:CheckBox, e:nexacro.CheckBoxChangeEventInfo)
{	
	if(this.gfn_isNull(this.Div05.cbo_NA_TRPL_C.value)){
		alert("거래처코드를 선택해 주세요.");
		this.Div05.chk_BILLING_AUTH.set_value("N");
