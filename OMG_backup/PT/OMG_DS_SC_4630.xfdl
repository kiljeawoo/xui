				preObj = "div_" + String(preNum).padLeft(2, "0");
				this.lookup(com).set_top(nexacro.toNumber(this.lookup(preObj).top) + nexacro.toNumber(this.lookup(preObj).height)-1);
			}
		}
	}
	
	this.resetScroll();
