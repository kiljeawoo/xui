this.fn_stlMethDsc= function(type)
{ 
	if(type =="1"){		
		this.Div05.Div03.cbo_BNK_C.set_index(this.ds_bankCode.findRow("SIMP_C","011"));		
		this.Div05.Div03.cbo_BNK_C.set_enable(true);		//거래은행
