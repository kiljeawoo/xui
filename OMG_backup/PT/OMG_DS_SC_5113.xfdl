// 	}
	/*비밀번호 validation검사*/
	if(!this.gfn_pwValidation(this.Div05.edt_MB_PW01.value)){
		return false;
	}			
	/*권한설정에 따른 이벤트 컨트롤*/	
	if(!this.fn_svcAuthValidation()){	
		return false;
	}
	
	/*권한조회 API호출*/
	this.fn_retrieveNaTrplCAuthLst();
	
