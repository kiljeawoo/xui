this.div_list_grd_master_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
    trace('currentcol' + obj.currentcol);
    
	 // 엑셀 클릭시만 실행
	
	var WRS_DTCFNM = ''; 
	var row = this.ds_dataM.rowposition;

