/* Call Back */
this.fn_callBack = function(svcID, ErrorCode, ErrorMsg)
{

	if(ErrorCode < 0)
	{
		this.gfn_getMessage("alert", "error.message.server.exception");
		return;
	}else{
		if(svcID == "selectWeekList"){			
            // alert('callback : ' + svcID);
            this.div_search.cbo_date.set_index(0);
		}
		

	}
}
