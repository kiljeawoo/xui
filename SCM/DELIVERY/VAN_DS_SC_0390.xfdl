/* Call Back */
this.fn_callBack = function(svcID, ErrorCode, ErrorMsg)
{
	if(ErrorCode < 0)
	{
		if(ErrorCode == "-99"){
			if(svcID == "getBizDtYn"){
				alert("입력하신 배송예정일은 휴무 입니다.");
				return;
			}
		}else{
			if(ErrorCode == "-999"){
				this.alert(ErrorMsg);
			}else{
				this.gfn_getMessage("alert", "error.message.server.exception");
			}
			return;
		}
		
	}else{
		if(svcID == "fn_checkPHD_FCLT_C"){
			trace("---------------"+this.ds_wrs.getColumn(0, "PHD_FCLT_C"));
			if(this.ds_wrs.rowcount != 0){
				trace(this.ds_wrs.rowcount);
				trace(var_cser_na_usr_sys_kdc);
				if ((this.gfn_trim(var_cser_na_usr_sys_kdc) =="52") || (this.gfn_trim(var_cser_na_usr_sys_kdc) =="54")) {
					
					var dvy_pla_qt 			= this.gfn_getNum(this.mae_dvy_pla_qt.value);//배송예정량
					var boxpe_aqz 			= this.gfn_getNum(this.gfn_nullToEmpty(this.mae_boxpe_aqz.value));//박스입수량			
