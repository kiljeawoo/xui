					var sCallbackFunc = "fn_callBack";//콜백
					var sTranType     = "S"; // U:저장/삭제, S:조회
					//API 호출
					this.gfn_callService(sSvcID, sURL, sInDatasets, 
					sOutDatasets, sArgument, sCallbackFunc, sTranType);
