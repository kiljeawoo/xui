				//this.div_list.div_page02.fn_pageSet(sTotal, sPagesize, "grd_detail, div_page02", lsNowPage);
			} else {
				this.div_list.grd_detail.set_nodatatext(this.gfn_getTextMessage('result.message.search.no.exist.data'));
