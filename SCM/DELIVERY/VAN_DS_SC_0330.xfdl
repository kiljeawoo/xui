this.VAN_DS_SC_0330_onkeydown = function(obj:Form, e:nexacro.KeyEventInfo)
{
	var key = this.gfn_onkeydown(e);

	if (key == "F") {
		this.btn_search_onclick();
	} else if (key == "E") {
		this.btn_excel_onclick();
	} else if (key == "P") {
		this.btn_print_onclick();
	}
}

